<%usersInfo.forEach(function(userInfoData){%>
    <tr>
        <td><%=userInfoData.name%></td>
        <td><%=userInfoData.email%></td>
        <td><%=userInfoData.gender%></td>
        <td><%=userInfoData.status%></td>
        <td>
        <a href="/userUpdate/<%=userInfoData.uid%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a href="/deleteUser/<%=userInfoData.uid%>" class="btn border-shadow delete"  >
            <span class="text-gradient"><i class="fas fa-trash"></i></span>
        </a>
    </td>
    </tr>
        
<%})%>